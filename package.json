{
  "name": "note-editor",
  "version": "0.0.1",
  "description": "",
  "main": "electron-main.js",
  "scripts": {
    "dev": "rm -rf node_modules/.cache && yarn run webpack-dev-server --colors --config ./webpack.dev.ts",
    "prod": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --colors --config ./webpack.prod.ts",
    "ts": "tsc electron-main.ts --lib esnext --skipLibCheck",
    "plugin": "ts-node build-asset.ts",
    "dev:electron": "yarn run ts && cross-env NODE_ENV=development yarn run electron .",
    "build:win": "yarn run prod && yarn run ts && electron-builder -w --dir && mkdir -p ./dist/win-unpacked/assets/audio && mkdir ./dist/win-unpacked/assets/musicGameSystems",
    "build:mac": "yarn run prod && yarn run ts && CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder -m --dir && mkdir -p ./dist/mac/assets/audio && mkdir ./dist/mac/assets/musicGameSystems",
    "build": "yarn run build:win && yarn run build:mac"
  },
  "license": "MIT",
  "devDependencies": {
    "@material-ui/core": "^4.8.1",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.37",
    "@types/classnames": "^2.2.7",
    "@types/config": "0.0.36",
    "@types/dat.gui": "^0.7.2",
    "@types/howler": "^2.1.0",
    "@types/lodash": "^4.14.145",
    "@types/mathjs": "^6.0.2",
    "@types/mousetrap": "^1.6.1",
    "@types/react": "^16.9.17",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.4",
    "@types/webpack-merge": "^4.1.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.2.0",
    "electron": "^4.1.0",
    "electron-builder": "^20.39.0",
    "electron-load-devtool": "^1.2.0",
    "file-loader": "^4.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "pixi.js": "^5.1.5",
    "ts-node": "^8.0.3",
    "typescript": "^3.7.2",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "config": "^3.2.4",
    "dat.gui": "^0.7.5",
    "fast-json-patch": "^3.0.0-1",
    "howler": "^2.1.1",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.13",
    "mathjs": "^6.2.3",
    "mobx": "^5.15.0",
    "mobx-react": "^6.1.4",
    "mobx-remotedev": "^0.3.6",
    "mousetrap": "^1.6.3",
    "notistack": "^0.9.5",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-color": "^2.17.0",
    "react-dom": "^16.12.0",
    "ts-loader": "^6.2.1"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "build": {
    "appId": "note-editor",
    "mac": {
      "category": "category.type"
    },
    "files": [
      "electron-main.js",
      "dist/index.html",
      "dist/dist/*"
    ]
  }
}
